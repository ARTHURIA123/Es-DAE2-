\maketitle
\section{}
\begin{equation}
    \lim_{x\to0}\frac{{\sqrt{x^2+100}-10}}{x^2}
    \end{equation}

\MakeUppercase{Solution}
\begin{equation}
     \lim_{x\to0}\frac{{\sqrt{x^2+100}-10}}{x^2}=\lim_{x\to0}\frac{{\sqrt{x^2+100}-10}}{x^2}.\frac{{\sqrt{x^2+100}+10}}{{\sqrt{x^2+100}+10}}
\end{equation}
\begin{equation}
    = \lim_{x\to0}\frac{{x^2}}{x^2(\sqrt{x^2+100}-10)}
\end{equation}
\begin{equation}
    = \lim_{x\to0}\frac{{1}}{(\sqrt{x^2+100}+10)}
\end{equation}
\begin{equation}
    = \frac{{1}}{(\sqrt{0+100}+10)}
\end{equation}
\begin{equation}
    = \frac{{1}}{20}
\end{equation}

\maketitle
\section{}
\begin{equation}
    \lim_{x\to9}\frac{{\sqrt{x}-3}}{x-9}
\end{equation}
\MakeUppercase{Solution}
\begin{equation}
\lim_{x\to9}\frac{{\sqrt{x}-3}}{x-9}=\lim_{x\to9}\frac{{x-9}}{x-9{(\sqrt{x}3})}
\end{equation}
\begin{equation}
=\lim_{x\to9}\frac{1}{{(\sqrt{x}+3})}
\end{equation}
\begin{equation}
=\frac{1}{6}
\end{equation}



